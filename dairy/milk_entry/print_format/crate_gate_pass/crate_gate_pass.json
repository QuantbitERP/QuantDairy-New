nformation</EM></CAPTION>-->\n<!--  <tr>-->\n<!--    <th>Crate Type</th>-->\n<!--    <th>Incoming Count</th>-->\n<!--    <th>Outgoing Count</th>-->\n<!--    <th>Damaged Count</th>-->\n<!--  </tr>-->\n\n<!--  {%- for row in doc.crate -%}-->\n<!-- <tr>-->\n<!--    <td>{{row.crate_type}}</td>-->\n<!--    <td>{{row.incoming_count}}</td>-->\n<!--    <td>{{row.outgoing_count}}</td>-->\n<!--    <td>{{row.damaged_count}}</td>-->\n<!--  </tr>-->\n<!--{%- endfor -%}-->\n<!--</table>-->\n\n\n<!--<hr>-->\n<!-- <table style=\"width:100%\"  border=\"1\">-->\n\n<!--  <tr>-->\n<!--    <th>Description</th>-->\n<!--    <th>Item</th>-->\n<!--    <th>Crate Type</th>-->\n<!--    <th>Quantity</th>-->\n\n<!--  </tr>-->\n\n<!--  {%- for row in doc.loose_crates -%}-->\n<!-- <tr>-->\n<!--    <td>{{row.description}}</td>-->\n<!--    <td>{{row.item_name}}</td>-->\n<!--    <td>{{row.crate_type}}</td>-->\n<!--    <td>{{row.qty}}</td>-->\n\n<!--  </tr>-->\n<!--{%- endfor -%}-->\n<!--</table>-->\n<hr>\n<pre class=\"tab\"><b>PREPARED BY</b>      <b>SHIFT INCHARGE</b>        <b>DISPATCHER</b>          <b>RECEIVED BY</b>     <b>SECURITY INCHARGE</b></pre>\n<pre><h5>{{frappe.get_fullname(doc.owner)}}</h5>     <b></b>        <b></b>          <b></b>     <b></b></pre>\n\n<!--<TABLE border=\"1\">-->\n<!--<CAPTION><EM>A test table with merged cells</EM></CAPTION>-->\n<!--<TR><TH rowspan=\"2\"><TH colspan=\"2\">Average-->\n<!--    <TH rowspan=\"2\">Red<BR>eyes-->\n<!--<TR><TH>height<TH>weight-->\n<!--<TR><TH>Males<TD>1.9<TD>0.003<TD>40%-->\n<!--<TR><TH>Females<TD>1.7<TD>0.002<TD>43%-->\n<!--</TABLE>-->",
